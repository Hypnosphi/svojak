{
  "name": "svojak",
  "version": "1.0.0",
  "repository": "https://github.com/Hypnosphi/svojak.git",
  "author": "Hypnosphi <Filipp.Riabchun@jetbrains.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8000"
  },
  "dependencies": {
    "eslint": "^5.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.16.4",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.0.3",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-markdown": "^4.0.6"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "all",
          "bracketSpacing": false
        }
      ]
    }
  }
}
